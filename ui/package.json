{
  "name": "football-goal-difference-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babelrc": "node .babelrc.js",
    "clean:client": "rimraf dist && mkdir -p dist",
    "clean:server": "rimraf server-dist && mkdir -p server-dist",
    "webpack:init": "npm-run-all clean:* babelrc",
    "webpack:client": "NODE_ENV=production webpack --config webpack.client.config.js --progress",
    "webpack:server": "NODE_ENV=production webpack --config webpack.server.config.js --progress",
    "build": "npm-run-all webpack:init webpack:client webpack:server",
    "start": "NODE_ENV=production node server-dist/server.js --colors",
    "webpack:client:dev": "NODE_ENV=development webpack --config webpack.client.config.js --debug --output-pathinfo --devtool inline-source-map",
    "webpack:server:dev": "NODE_ENV=development webpack --config webpack.server.config.js --debug --output-pathinfo --devtool inline-source-map",
    "webpack:dev": "npm-run-all webpack:init webpack:*:dev",
    "webpack:watch": "npm-run-all --parallel 'webpack:*:dev -- --watch'",
    "start:dev": "APP_ENV=development NODE_ENV=development nodemon --watch dist --watch server-dist --ext js server-dist/server.js",
    "development": "npm run webpack:dev && npm run start",
    "dev": "npm run development"
  },
  "author": "quirkles",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "history": "latest",
    "immutable": "^3.8.2",
    "prop-types": "latest",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "serialize-javascript": "latest"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.9.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "latest",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
